<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:odk="http://www.opendatakit.org/xforms" xmlns:cht="https://communityhealthtoolkit.org">
  <h:head>
    <h:title>Resolve Report Task</h:title>
    <model odk:xforms-version="1.0.0">
      <itext>
        <translation lang="English (en)">
          <text id="/data/submit:label">
            <value> This task was triggered with the following input data:

- Task Id: &lt;code&gt;<output value=" /data/inputs/task_id "/>&lt;/code&gt;
- Contact: &lt;a target="_self" href="#" rel="noopener" class="dynamic-url"&gt; <output value=" /data/inputs/contact/name "/> &lt;div class="url hidden"&gt;/#/contacts/<output value=" /data/patient_uuid "/>&lt;/div&gt; &lt;/a&gt;
- Source: &lt;code&gt;<output value=" /data/inputs/source "/>&lt;/code&gt;
- Source Id: &lt;a target="_self" href="#" rel="noopener" class="dynamic-url"&gt; <output value=" /data/inputs/source_id "/> &lt;div class="url hidden"&gt;/#/reports/<output value=" /data/inputs/source_id "/>&lt;/div&gt; &lt;/a&gt; 
- Source Form: &lt;code&gt;<output value=" /data/inputs/from_modify_content/source_form "/>&lt;/code&gt;
- Event Id: &lt;code&gt;<output value=" /data/inputs/from_modify_content/event_id "/>&lt;/code&gt;

Are you sure you want to resolve the task?</value>
          </text>
          <text id="/data/submit:hint">
            <value>Submitting this form will resolve the associated task</value>
          </text>
        </translation>
      </itext>
      <instance>
        <data id="resolve_report_task" version="20250213195558" prefix="J1!resolve_report_task!" delimiter="#">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <source/>
            <source_id/>
            <contact>
              <_id/>
              <name/>
            </contact>
            <task_id/>
            <from_modify_content>
              <event_id/>
              <source_form/>
            </from_modify_content>
          </inputs>
          <patient_uuid/>
          <submit/>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </data>
      </instance>
      <instance id="contact-summary"/>
      <instance id="user-contact-summary"/>
      <bind nodeset="/data/inputs" relevant="false()"/>
      <bind nodeset="/data/inputs/source" type="string"/>
      <bind nodeset="/data/inputs/source_id" type="string"/>
      <bind nodeset="/data/inputs/contact/_id" type="string"/>
      <bind nodeset="/data/inputs/contact/name" type="string"/>
      <bind nodeset="/data/inputs/task_id" type="string"/>
      <bind nodeset="/data/inputs/from_modify_content/event_id" type="string"/>
      <bind nodeset="/data/inputs/from_modify_content/source_form" type="string"/>
      <bind nodeset="/data/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/data/submit" readonly="true()" type="string"/>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" jr:preload="uid"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group ref="/data/inputs">
      <input ref="/data/inputs/source"></input>
      <input ref="/data/inputs/source_id"></input>
      <group ref="/data/inputs/contact">
        <input ref="/data/inputs/contact/_id"></input>
        <input ref="/data/inputs/contact/name"></input>
      </group>
      <input ref="/data/inputs/task_id"></input>
      <group ref="/data/inputs/from_modify_content">
        <input ref="/data/inputs/from_modify_content/event_id"></input>
        <input ref="/data/inputs/from_modify_content/source_form"></input>
      </group>
    </group>
    <input ref="/data/submit">
      <label ref="jr:itext('/data/submit:label')"/>
      <hint ref="jr:itext('/data/submit:hint')"/>
    </input>
  </h:body>
</h:html>