<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:cht="https://communityhealthtoolkit.org">
  <h:head>
    <h:title>With Extended Select</h:title>
    <model>
      <itext>
        <translation lang="English (en)">
          <text id="/with_extended_select/page/select_0:label">
            <value>First Select</value>
          </text>
          <text id="/with_extended_select/page/select_1:label">
            <value>Second Select</value>
          </text>
        </translation>
      </itext>
      <instance>
        <with_extended_select id="with_extended_select" prefix="J1!with_extended_select!" delimiter="#" version="20250402122914">
          <page>
            <select_0/>
            <select_1/>
          </page>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </with_extended_select>
      </instance>
      <instance id="contact-summary"/>
      <bind nodeset="/with_extended_select/page/select_0" type="string" calculate="cht:extension-lib(&quot;extended-select-widget.js&quot;, &quot;/with_extended_select/page/select_0&quot;)"/>
      <bind nodeset="/with_extended_select/page/select_1" type="string" calculate="cht:extension-lib(&quot;extended-select-widget.js&quot;, &quot;/with_extended_select/page/select_1&quot;)"/>
      <bind nodeset="/with_extended_select/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/with_extended_select/page">
      <input ref="/with_extended_select/page/select_0">
        <label ref="jr:itext('/with_extended_select/page/select_0:label')"/>
      </input>
      <input ref="/with_extended_select/page/select_1">
        <label ref="jr:itext('/with_extended_select/page/select_1:label')"/>
      </input>
    </group>
  </h:body>
</h:html>
