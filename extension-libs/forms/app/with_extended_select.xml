<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:odk="http://www.opendatakit.org/xforms" xmlns:cht="https://communityhealthtoolkit.org">
  <h:head>
    <h:title>With Extended Select</h:title>
    <model odk:xforms-version="1.0.0">
      <itext>
        <translation lang="English (en)">
          <text id="/data/page:label">
            <value></value>
          </text>
          <text id="/data/page/select_0:label">
            <value>First Select</value>
          </text>
          <text id="/data/page/select_1:label">
            <value>Second Select</value>
          </text>
        </translation>
      </itext>
      <instance>
        <data id="with_extended_select" version="20250402122914" prefix="J1!with_extended_select!" delimiter="#">
          <page>
            <select_0></select_0>
            <select_1></select_1>
          </page>
          <meta tag="hidden">
            <instanceID></instanceID>
          </meta>
        </data>
      </instance>
      <instance id="contact-summary"></instance>
      <instance id="user-contact-summary"></instance>
      <bind nodeset="/data/page/select_0" type="string" calculate="cht:extension-lib(&quot;extended-select-widget.js&quot;, &quot;/with_extended_select/page/select_0&quot;)"></bind>
      <bind nodeset="/data/page/select_1" type="string" calculate="cht:extension-lib(&quot;extended-select-widget.js&quot;, &quot;/with_extended_select/page/select_1&quot;)"></bind>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" jr:preload="uid"></bind>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/data/page">
      <label ref="jr:itext('/data/page:label')"></label>
      <input ref="/data/page/select_0">
        <label ref="jr:itext('/data/page/select_0:label')"></label>
      </input>
      <input ref="/data/page/select_1">
        <label ref="jr:itext('/data/page/select_1:label')"></label>
      </input>
    </group>
  </h:body>
</h:html>